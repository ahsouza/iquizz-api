    </style>
<body>
  <header>
            <!-- Left aligned menu below button -->
  <button id="demo-menu-lower-left"
    class="mdl-button mdl-js-button mdl-button--icon">
    <i class="material-icons">
      more_vert
    </i>
  </button>

  <ul class="mdl-menu mdl-menu--bottom-left mdl-js-menu mdl-js-ripple-effect"
    for="demo-menu-lower-left">        
    <a href="/api/dashboard">
      <li class="mdl-menu__item mdl-menu__item--full-bleed-divider"><i class="material-icons">arrow_back_ios</i></li>
    </a>
  </ul>
  </header>

    <!-- Square card -->
    <style>
    .demo-card-square.mdl-card {
      width: 220px;
      height: 120px;
    }
    .demo-card-square > .mdl-card__title {
      color: #fff;
      background:
        url('../assets/demos/dog.png') bottom right 15% no-repeat #46B6AC;
    }
    </style>




<div class="mdl-grid">


<% if (account.disciplines) { %>
  <% for (account.discipline of account.disciplines ) { %>
  
  <div class="mdl-cell mdl-cell--2-col" style="background-color: pink">
    
    <div class="demo-card-square mdl-card mdl-shadow--2dp">
      <div class="mdl-card__title mdl-card--expand">
        <h2 class="mdl-card__title-text"><%= account.discipline.nameDiscipline %></h2>
      </div>
      <div class="mdl-card__supporting-text">
        <b> <%= account.discipline.act %></b>
        <br/><br/>
        <b>NÃ­vel:</b> <%= account.discipline.levelLearning %>
      </div>
      <div class="mdl-card__actions mdl-card--border">
        <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
          INICIAR
        </a>
      </div>
    </div>

  </div>

  <% } %>

<% } %>

</div>



