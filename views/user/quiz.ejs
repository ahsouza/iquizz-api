function move(x) {
  var elem = document.getElementById("myBar");  
  var width = 10;
  var id = setInterval(frame, 30);
  function frame() {
    if (width >= (x *10)) {
      clearInterval(id);
    } else {
      width++; 
      elem.style.width = width + '%'; 
      elem.innerHTML = width * 1  + '%';
    }
  }
}

function moveError(y) {
  var elem = document.getElementById("myBar-erros");   
  var width = 10;
  var id = setInterval(frame, 30);
  function frame() {
    if (width >= (y *10)) {
